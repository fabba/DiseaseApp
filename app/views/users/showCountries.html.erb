<div id="map1" class="map1">
  
</div>
<div style="left:5%;position:absolute;background-color:#66C2A5;">
    Visited
</div>
<div style="left:5%;top:10%;position:absolute;background-color:#FFA500;">
    Wish
</div>
<script>

  var series = {
        regions: [{
          values: {
          <% @countriesVisited.each do |visit| %>
          <%= visit.code%>: "#66C2A5",
          <% end %>
          <% @countriesWish.each do |wish| %>
          <%= wish.code%>: "#FFA500",
          <% end %>},
          attribute: 'fill' 
        }]
      };
      
</script>

